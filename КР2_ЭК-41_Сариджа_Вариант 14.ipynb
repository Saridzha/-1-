   "source": [
    "def transposed(array):\n",
    "    T = []\n",
    "    for i in range(len(array[0])):\n",
    "        t = []\n",
    "        for row in array:\n",
    "            t.append(row[i])\n",
    "        T.append(t)\n",
    "    return T"
   ]
